{
  "name": "vscode-runkit",
  "displayName": "vscode-runkit",
  "description": "Create runkit notebooks within VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": ["onNotebook:open-notebook"],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebooks": [
      {
        "type": "runkit-notebook",
        "displayName": "Runkit Notebook",
        "selector": [
            {
              "filenamePattern": "*.{jsr,rjs,runkit}"
            }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "vscode-runkit",
        "entrypoint": "./out/client/index.js",
        "displayName": "vscode-runkit",
        "mimeTypes": [
          "x-application/custom-json-output",
          "x-application/sample-json-renderer"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/vscode": "^1.71.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^2.1.5",
    "util": "^0.12.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
